{
  "version": "0.2.0",
  "configurations": [
    
    {
      "type": "node",
      "request": "launch",
      "name": "Netlify Functions Only (localhost)",
      "program": "${workspaceFolder}/node_modules/.bin/netlify",
      "args": [
        "functions:serve",
        "${workspaceFolder}/netlify/functions"
      ],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "console": "integratedTerminal"
    }
  ]
}
